    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-resize-panel": "^0.3.5",
    "react-scripts": "^3.4.1",
    "react-tooltip": "^4.2.10",
